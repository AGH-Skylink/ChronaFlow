{
  "expo": {
    "name": "ChronaFlow",
    "slug": "ChronaFlow",
    "version": "0.0.2",
    "orientation": "portrait",
    "icon": "./assets/images/chronaflow.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/chronaflow.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.skylink.chronaflow",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/chronaflow.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.skylink.chronaflow"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "6337325b-643a-4bf8-9781-0e4efca00dee"
      }
    }
  }
}
