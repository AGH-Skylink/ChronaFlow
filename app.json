{
  "expo": {
    "name": "ChronaFlow",
    "slug": "ChronaFlow",
    "version": "0.0.2",
    "orientation": "portrait",
    "icon": "./assets/images/chronaflow.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/chronaflow.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/chronaflow.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.skylink.chronaflow"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": ["expo-router"],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "93c0d3c0-d4ca-414e-a82a-6535448e65b3"
      }
    }
  }
}
